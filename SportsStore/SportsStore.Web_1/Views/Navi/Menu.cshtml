


@model IEnumerable<string>
@foreach (string link in Model)
{

    <li class="nav-item">
        @Html.RouteLink(link, new
   {
       controller = "Product",
       action = "List",
       category = link,
       page = 1
   }, new { @class = link == ViewBag.SelectedCategory ? "selected" : null })



    </li>
}



